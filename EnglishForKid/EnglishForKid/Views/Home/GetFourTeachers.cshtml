@using EnglishForKid.Models.ViewModels;

@{
    List<UserReturnModel> users = ViewBag.Teachers;
    if (users == null)
    {
        users = new List<UserReturnModel>();
    }
    foreach (UserReturnModel user in users)
    {
        <div class="col-md-3">
            <div class="wow bounceInUp" data-wow-delay="0.2s">
                <div class="team boxed-grey">
                    <div class="inner">
                        <h5>@user.FullName</h5>

                        <div class="avatar">
                            <img src="@user.Avatar" alt="" class="img-responsive img-circle" />
                        </div>

                    </div>
                </div>
                <div class="maskingContent">
                    <ul class="list-inline">
                        <li><a href="#"><i class="fa fa-facebook " aria-hidden="true"></i></a></li>
                        <li><a href="#"><i class="fa fa-twitter" aria-hidden="true"></i></a></li>
                        <li><a href="#"><i class="fa fa-google-plus" aria-hidden="true"></i></a></li>
                        <li><a href="#"><i class="fa fa-pinterest-p" aria-hidden="true"></i></a></li>
                    </ul>
                </div>
            </div>
        </div>
    }
}