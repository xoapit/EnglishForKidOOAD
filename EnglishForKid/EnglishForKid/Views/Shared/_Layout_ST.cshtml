
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="">

    <title>English for kids</title>

    <!-- Bootstrap Core CSS -->
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <!-- Fonts -->
    <link href="~/Content/font-awesome/css/font-awesome.min.css" rel="stylesheet" />

    <script src="~/Content/js/jquery-1.10.2.js"></script>
    <script src="~/Content/js/jquery.min.js"></script>

    <link href="~/Content/css/header.css" rel="stylesheet" />
    <link href="~/Content/css/myStyle.css" rel="stylesheet" />

    <link href="https://fonts.googleapis.com/css?family=Architects+Daughter|Gochi+Hand|Pacifico&amp;subset=latin-ext,vietnamese" rel="stylesheet">
    <script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>
    <script src="~/Content/js/bootstrap.min.js"></script>
</head>

<body>
    @{
        Html.RenderPartial("TopBarView");
    }
    <div class="container-fluid" style="margin-top:60px;">

        <div class="row">
            <div class="col-xs-12 col-md-10 col-md-offset-1">
                <div class="row">
                    <div class="col-md-12">
                        <img src="~/Content/files/image/banner.jpg" style="background-size: cover; width: 100%;height: 100% "/>
                    </div>
                </div>
                <div class="row">
                    <div class="col-sm-12 col-md-8"> @RenderBody()</div>
                    <div class="col-sm-12 col-md-4">
                        @{
                            Html.RenderPartial("Survey");
                        }
                    </div>
                </div>
            </div>
        </div>
    </div>
    @{
        Html.RenderPartial("footer");
    }


    <!-- Core JavaScript Files -->


    <script src="~/Content/js/jquery.easing.min.js"></script>
    <script src="~/Content/js/wow.min.js"></script>
    <script src="~/Content/js/jquery.scrollTo.js"></script>

    <!-- Custom Theme JavaScript -->

    <script src="~/Content/js/custom.js"></script>

</body>

</html>
